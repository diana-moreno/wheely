{
  "name": "wheely-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "http-server": "^0.12.0",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "wheely-utils": "file:../wheely-utils"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "sass": "^1.64.2",
    "wheely-data": "file:../wheely-data"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test --runInBand --no-cache",
    "test-debug": "react-scripts inspect test --runInBand --no-cache",
    "sass": "sass template/index.sass template/index.css",
    "sass:watch": "sass --watch template/index.sass template/index.css",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
